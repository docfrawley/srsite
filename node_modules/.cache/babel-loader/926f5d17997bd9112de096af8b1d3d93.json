wMod.value.description)\n            const { error: theerror, document } = await getLesson(whichcourse.value, theMod, 1)\n           if (!theerror.value){\n               newVidMod.value =  {...document.value}\n               console.log('got the new video: ', newVidMod)\n           }\n            // if (vids){\n                \n            //     const newVid = vids.value.filter((vid) => {\n            //         console.log(\"here we go: \", vid)\n            //         // return vid.order == 1\n            //     })\n            //     newVidMod.value = { ...newVid[0] }\n            //     console.log(\"here is the new video: \", newVidMod.value)\n            // }\n            \n            \n        }\n\n        \n\n        const returnToAll=()=>{\n            whichVid.value={}\n            theNewMod.value=null\n        }\n\n        \n\n        return { whichcourse, error, documents, showvidInfo, whichVid, returnToAll, moveModule, theNewMod, newVidMod}\n    }\n}\n</script>\n\n<style scoped>\n    .module-list{\n        display:flex;\n        flex-direction: row;\n        justify-content: space-around;\n    }\n    .ind-mod {\n        display: inline-block;\n        cursor: pointer;\n    }\n    .next-previous {\n        display: flex;\n        justify-content: center;\n    }\n    .np-module {\n        cursor: pointer;\n        margin: 0 15px;\n    }\n\n</style>"]},"metadata":{},"sourceType":"module"}