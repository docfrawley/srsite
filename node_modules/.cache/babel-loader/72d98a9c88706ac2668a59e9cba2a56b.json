nts/SingleModel.vue'\nimport { numberLiteralTypeAnnotation } from '@babel/types'\n\nexport default {\n    name: 'Courseview',\n    components: { ShowModule, SingleModel },\n    props: ['course'],\n    setup(props) {\n        const { error, documents } = getOrderDocs('course-modules', 'course', props.course)\n        const whichcourse = ref(props.course)\n        const whichVid=ref({})\n        const theNewMod = ref()\n        const showvidInfo = (video) =>{\n            whichVid.value = video\n            whichVid.value.course = props.course\n           \n        }\n\n        const moveModule =(theMod)=>{\n            const newModule= documents.value.filter((document)=>{\n                return document.module == theMod\n              \n            })\n            theNewMod.value = newModule\n            console.log(\"here is the new module: \", newModule.title)\n        }\n\n        \n\n        const returnToAll=()=>{\n            whichVid.value={}\n        }\n\n        \n\n        return { whichcourse, error, documents, showvidInfo, whichVid, returnToAll, moveModule}\n    }\n}\n</script>\n\n<style scoped>\n    .module-list{\n        display:flex;\n        flex-direction: row;\n        justify-content: space-around;\n    }\n    .ind-mod {\n        display: inline-block;\n        cursor: pointer;\n    }\n    .next-previous {\n        display: flex;\n        justify-content: center;\n    }\n    .np-module {\n        cursor: pointer;\n        margin: 0 15px;\n    }\n\n</style>"]},"metadata":{},"sourceType":"module"}