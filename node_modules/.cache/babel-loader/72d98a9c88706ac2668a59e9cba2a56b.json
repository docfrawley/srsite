{"ast":null,"code":"import ShowModule from '@/components/ShowModule.vue';\nimport getOrderDocs from '@/composables/getOrderDocs';\nimport { ref } from 'vue';\nimport SingleModel from '@/components/SingleModel.vue';\nimport { numberLiteralTypeAnnotation } from '@babel/types';\nexport default {\n  name: 'Courseview',\n  components: {\n    ShowModule,\n    SingleModel\n  },\n  props: ['course'],\n\n  setup(props) {\n    const {\n      error,\n      documents\n    } = getOrderDocs('course-modules', 'course', props.course);\n    const whichcourse = ref(props.course);\n    const whichVid = ref({});\n    const theNewMod = ref();\n\n    const showvidInfo = video => {\n      whichVid.value = video;\n      whichVid.value.course = props.course;\n    };\n\n    const moveModule = theMod => {\n      const newModule = documents.value.filter(document => {\n        return document.module == theMod;\n      });\n      theNewMod.value = newModule;\n      console.log(\"here is the new module: \", newModule.title);\n    };\n\n    const returnToAll = () => {\n      whichVid.value = {};\n    };\n\n    return {\n      whichcourse,\n      error,\n      documents,\n      showvidInfo,\n      whichVid,\n      returnToAll,\n      moveModule\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA4BA,OAAOA,UAAP,MAAuB,6BAAvB;AACA,OAAOC,YAAP,MAAyB,4BAAzB;AACA,SAASC,GAAT,QAAoB,KAApB;AACA,OAAOC,WAAP,MAAwB,8BAAxB;AACA,SAASC,2BAAT,QAA4C,cAA5C;AAEA,eAAe;EACXC,IAAI,EAAE,YADK;EAEXC,UAAU,EAAE;IAAEN,UAAF;IAAcG;EAAd,CAFD;EAGXI,KAAK,EAAE,CAAC,QAAD,CAHI;;EAIXC,KAAK,CAACD,KAAD,EAAQ;IACT,MAAM;MAAEE,KAAF;MAASC;IAAT,IAAuBT,YAAY,CAAC,gBAAD,EAAmB,QAAnB,EAA6BM,KAAK,CAACI,MAAnC,CAAzC;IACA,MAAMC,WAAU,GAAIV,GAAG,CAACK,KAAK,CAACI,MAAP,CAAvB;IACA,MAAME,QAAQ,GAACX,GAAG,CAAC,EAAD,CAAlB;IACA,MAAMY,SAAQ,GAAIZ,GAAG,EAArB;;IACA,MAAMa,WAAU,GAAKC,KAAD,IAAU;MAC1BH,QAAQ,CAACI,KAAT,GAAiBD,KAAjB;MACAH,QAAQ,CAACI,KAAT,CAAeN,MAAf,GAAwBJ,KAAK,CAACI,MAA9B;IAEJ,CAJA;;IAMA,MAAMO,UAAS,GAAIC,MAAD,IAAU;MACxB,MAAMC,SAAS,GAAEV,SAAS,CAACO,KAAV,CAAg