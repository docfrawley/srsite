          \n                await console.log(\"document: \", document)\n                whichVid.value.iframe = await document.iframe\n                whichVid.value.module = await document.module\n                whichVid.value.course = whichcourse.value\n            \n               \n               \n           \n            \n        }\n\n        \n\n        const returnToAll=()=>{\n            whichVid.value=null\n            theNewMod.value=null\n        }\n\n        \n\n        return { whichcourse, error, documents, showvidInfo, whichVid, returnToAll, moveModule}\n    }\n}\n</script>\n\n<style scoped>\n    .module-list{\n        display:flex;\n        flex-direction: row;\n        justify-content: space-around;\n    }\n    .ind-mod {\n        display: inline-block;\n        cursor: pointer;\n    }\n    .next-previous {\n        display: flex;\n        justify-content: center;\n    }\n    .np-module {\n        cursor: pointer;\n        margin: 0 15px;\n    }\n\n</style>"]},"metadata":{},"sourceType":"module"}