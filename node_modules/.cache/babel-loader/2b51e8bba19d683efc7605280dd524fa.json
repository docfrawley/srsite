{"ast":null,"code":"import ShowModule from '@/components/ShowModule.vue';\nimport getOrderDocs from '@/composables/getOrderDocs';\nimport { ref } from 'vue';\nimport SingleModel from '@/components/SingleModel.vue';\nexport default {\n  name: 'Courseview',\n  components: {\n    ShowModule,\n    SingleModel\n  },\n  props: ['course'],\n\n  setup(props) {\n    const {\n      error,\n      documents\n    } = getOrderDocs('course-modules', 'course', props.course);\n    const whichcourse = ref(props.course);\n    const whichVid = ref({});\n    const theNewMod = ref(null);\n    const newVidMod = ref(null);\n\n    const showvidInfo = video => {\n      whichVid.value = video;\n      whichVid.value.course = props.course;\n    };\n\n    const moveModule = theMod => {\n      const newModule = documents.value.filter(document => {\n        return document.module == theMod;\n      });\n      theNewMod.value = { ...newModule[0]\n      };\n      console.log(\"here is the new module: \", theNewMod.value.description);\n      const {\n        error,\n        documents: videos\n      } = getOrderDocs(whichcourse.value, 'module', theMod);\n      console.log(\"new set of videos: \", videos);\n\n      if (videos) {\n        const newVid = videos.value.filter(vid => {\n          console.log(\"here we go: \", vid.order == 1, vid.order);\n          return vid.order == 1;\n        });\n        newVidMod.value = { ...newVid[0]\n        };\n        console.log(\"here is the new video: \", newVidMod.value);\n      }\n    };\n\n    const returnToAll = () => {\n      whichVid.value = {};\n      theNewMod.value = null;\n    };\n\n    return {\n      whichcourse,\n      error,\n      documents,\n      showvidInfo,\n      whichVid,\n      returnToAll,\n      moveModule,\n      theNewMod\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA+BA,OAAOA,UAAP,MAAuB,6BAAvB;AACA,OAAOC,YAAP,MAAyB,4BAAzB;AACA,SAASC,GAAT,QAAoB,KAApB;AACA,OAAOC,WAAP,MAAwB,8BAAxB;AAEA,eAAe;EACXC,IAAI,EAAE,YADK;EAEXC,UAAU,EAAE;IAAE