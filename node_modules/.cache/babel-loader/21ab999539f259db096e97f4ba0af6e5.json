{"ast":null,"code":"import getOrderDocs from '@/composables/getOrderDocs';\nimport getLesson from '@/composables/getLesson';\nimport ShowVidDetails from '@/components/ShowVidDetails.vue';\nimport { ref } from '@vue/reactivity';\nexport default {\n  props: ['specifics'],\n  components: {\n    ShowVidDetails: ShowVidDetails\n  },\n  setup: function setup(props) {\n    var theVideo = getLesson(props.specifics.course, props.specifics.module, props.specifics.order);\n\n    var _getOrderDocs = getOrderDocs(props.specifics.course, 'module', props.specifics.module),\n        error = _getOrderDocs.error,\n        videos = _getOrderDocs.documents;\n\n    var newVideo = function newVideo(specs) {\n      theVideo.value = specs.vidinfo;\n    };\n\n    return {\n      theVideo: theVideo,\n      videos: videos,\n      newVideo: newVideo\n    };\n  }\n};","map":{"version":3,"mappings":"AAoBA,OAAOA,YAAP,MAAyB,4BAAzB;AACA,OAAOC,SAAP,MAAsB,yBAAtB;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,SAASC,GAAT,QAAoB,iBAApB;AAEA,eAAe;EACXC,KAAK,EAAE,CAAC,WAAD,CADI;EAEXC,UAAU,EAAE;IAAEH,cAAa,EAAbA;EAAF,CAFD;EAGXI,KAHW,iBAGLF,KAHK,EAGC;IACR,IAAMG,QAAO,GAAKN,SAAS,CAACG,KAAK,CAACI,SAAN,CAAgBC,MAAjB,EAAyBL,KAAK,CAACI,SAAN,CAAgBE,MAAzC,EAAiDN,KAAK,CAACI,SAAN,CAAgBG,KAAjE,CAA3B;;IACA,oBAAqCX,YAAY,CAACI,KAAK,CAACI,SAAN,CAAgBC,MAAjB,EAAyB,QAAzB,EAAmCL,KAAK,CAACI,SAAN,CAAgBE,MAAnD,CAAjD;IAAA,IAAQE,KAAR,iBAAQA,KAAR;IAAA,IAA0BC,MAA1B,iBAAeC,SAAf;;IAEA,IAAMC,QAAO,GAAG,SAAVA,QAAU,CAACC,KAAD,EAAW;MACvBT,QAAQ,CAACU,KAAT,GAAiBD,KAAK,CAACE,OAAvB;IACJ,CAFA;;IAIA,OAAO;MAAEX,QAAQ,EAARA,QAAF;MAAYM,MAAM,EAANA,MAAZ;MAAoBE,QAAQ,EAARA;IAApB,CAAP;EACJ;AAZW,CAAf","names":["getOrderDocs","getLesson","ShowVidDetails","ref","props","components","setup","theVideo","specifics","course","module","order","error","videos","documents","newVideo","specs","value","vidinfo"],"sourceRoot":"","sources":["/Users/docfrawley/Sites/srsite/src/components/SingleModel.vue"],"sourcesContent":["<template>\n    <div v-if=\"theVideo\">\n        <div v-html=