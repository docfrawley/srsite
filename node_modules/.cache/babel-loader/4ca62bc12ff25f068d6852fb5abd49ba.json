{"ast":null,"code":"import { ref, reactive } from 'vue';\nimport getCollection from '@/composables/getCollection';\nimport CourseForm from '@/components/CourseForm.vue';\nimport ModuleForm from '@/components/ModuleForm.vue';\nimport VideoForm from '@/components/VideoForm.vue';\nexport default {\n  components: {\n    CourseForm: CourseForm,\n    ModuleForm: ModuleForm,\n    VideoForm: VideoForm\n  },\n  setup: function setup() {\n    var currentCourse = ref();\n    var currentModule = ref();\n    var currentVideo = ref();\n    var allvideos = ref();\n    var showCourse = ref(false);\n\n    var _getCollection = getCollection('courses'),\n        courses = _getCollection.documents;\n\n    var goToCourse = function goToCourse(c) {\n      showCourse.value = false;\n      currentCourse.value = null;\n\n      if (c) {\n        currentCourse.value = c;\n        showCourse.value = true;\n        console.log(\"currentCourse: \", currentCourse.value);\n      }\n    };\n\n    var showVid = function showVid(info) {\n      if (info) {\n        currentVideo.value = info;\n      }\n    };\n\n    var newCourseForm = function newCourseForm() {\n      currentCourse.value = \"new_course\";\n    };\n\n    return {\n      showCourse: showCourse,\n      goToCourse: goToCourse,\n      courses: courses,\n      currentCourse: currentCourse,\n      currentModule: currentModule,\n      showVid: showVid,\n      newCourseForm: newCourseForm,\n      allvideos: allvideos,\n      currentVideo: currentVideo\n    };\n  }\n};","map":{"version":3,"mappings":"AAwCA,SAASA,GAAT,EAAcC,QAAd,QAA8B,KAA9B;AACA,OAAOC,aAAP,MAA0B,6BAA1B;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AACA,OAAOC,SAAP,MAAsB,4BAAtB;AAEA,eAAe;EACbC,UAAU,EAAE;IAACH,UAAU,EAAVA,UAAD;IAAaC,UAAU,EAAVA,UAAb;IAAyBC,SAAQ,EAARA;EAAzB,CADC;EAEdE,KAFc,mBAEN;IACP,IAAMC,aAAY,GAAIR,GAAG,EAAzB;IACA,IAAMS,aAAY,GAAIT,GAAG,EAAzB;IACA,IAAMU,YAAW,GAAIV,GAAG,EAAxB;IACA,IAAMW,SAAQ,GAAIX,GAAG,EAArB;IACA,IAAMY,UAAU,GAACZ,GAAG,CAAC,KAAD,CAApB;;IACA