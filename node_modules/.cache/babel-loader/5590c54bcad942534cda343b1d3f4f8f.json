     <span class=\"ind-mod\" @click=\"moveModule(document.module)\">{{document.title}} </span>\n            </div>\n        </div>\n        <div v-if=\"theNewMod\">\n        </div>\n        <SingleModel :specifics=\"whichVid\" />\n        <div class=\"next-previous\">\n            <div class=\"np-module\" v-if=\"whichVid.module>1\" @click=\"moveModule(whichVid.module-1)\">Previous Module</div>\n            <div class=\"np-module\" v-if=\"whichVid.module<documents.length\" @click=\"moveModule(whichVid.module+1)\">Next\n                Module</div>\n        </div>\n\n    </div>\n\n    <div v-else>\n        <div v-for=\"document in documents\" :key=\"document.id\">\n            <ShowModule :document=\"document\" @vidInfo=\"showvidInfo\" />\n        </div>\n    </div>\n\n</template>\n\n<script>\nimport ShowModule from '@/components/ShowModule.vue'\nimport getOrderDocs from '@/composables/getOrderDocs'\nimport { ref } from 'vue'\nimport SingleModel from '@/components/SingleModel.vue'\n\nexport default {\n    name: 'Courseview',\n    components: { ShowModule, SingleModel },\n    props: ['course'],\n    setup(props) {\n        const { error, documents } = getOrderDocs('course-modules', 'course', props.course)\n        const whichcourse = ref(props.course)\n        const whichVid=ref({})\n        const theNewMod = ref(null)\n        const newVidMod = ref(null)\n        const showvidInfo = (video) =>{\n            whichVid.value = video\n            whichVid.value.course = props.course\n           \n        }\n\n        const moveModule =(theMod)=>{\n            // const newModule= documents.value.filter((document)=>{\n            //     return document.module == theMod\n              \n            // })\n            // theNewMod.value = {...newModule[0]}\n            // console.log(\"here is the new module: \", theNewMod.value.description)\n            const { error, documents: videos } = getOrderDocs(whichcourse.value, 'module', theMod)\n            console.log(\"new set of videos: \", videos)\n            if (videos){\n                \n                const newVid = videos.value.filter((vid) => {\n                    console.log(\"here we go: \", vid.order == 1, vid.order)\n                    return vid.order == 1\n                })\n                newVidMod.value = { ...newVid[0] }\n                console.log(\"here is the new video: \", newVidMod.value)\n            }\n            \n            \n        }\n\n        \n\n        const returnToAll=()=>{\n            whichVid.value={}\n            theNewMod.value=null\n        }\n\n        \n\n        return { whichcourse, error, documents, showvidInfo, whichVid, returnToAll, moveModule, theNewMod}\n    }\n}\n</script>\n\n<style scoped>\n    .module-list{\n        display:flex;\n        flex-direction: row;\n        justify-content: space-around;\n    }\n    .ind-mod {\n        display: inline-block;\n        cursor: pointer;\n    }\n    .next-previous {\n        display: flex;\n        justify-content: center;\n    }\n    .np-module {\n        cursor: pointer;\n        margin: 0 15px;\n    }\n\n</style>"]},"metadata":{},"sourceType":"module"}