{"ast":null,"code":"import ShowModule from '@/components/ShowModule.vue';\nimport getOrderDocs from '@/composables/getOrderDocs';\nimport { ref } from 'vue';\nimport SingleModel from '@/components/SingleModel.vue';\nexport default {\n  name: 'Courseview',\n  components: {\n    ShowModule,\n    SingleModel\n  },\n  props: ['course'],\n\n  setup(props) {\n    const {\n      error,\n      documents\n    } = getOrderDocs('course-modules', 'course', props.course);\n    const whichcourse = ref(props.course);\n    const whichVid = ref({});\n\n    const showvidInfo = video => {\n      whichVid.value = video;\n      console.log(\"here is the info: \", whichVid.value);\n      whichVid.value.course = props.course;\n    };\n\n    const moveModule = theMod => {\n      console.log(\"Which module now: \", theMod);\n      const newModule = documents.filter(document => {\n        document.value.module == theMod;\n      });\n    };\n\n    const returnToAll = () => {\n      whichVid.value = {};\n    };\n\n    return {\n      whichcourse,\n      error,\n      documents,\n      showvidInfo,\n      whichVid,\n      returnToAll,\n      moveModule\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA4BA,OAAOA,UAAP,MAAuB,6BAAvB;AACA,OAAOC,YAAP,MAAyB,4BAAzB;AACA,SAASC,GAAT,QAAoB,KAApB;AACA,OAAOC,WAAP,MAAwB,8BAAxB;AAEA,eAAe;EACXC,IAAI,EAAE,YADK;EAEXC,UAAU,EAAE;IAAEL,UAAF;IAAcG;EAAd,CAFD;EAGXG,KAAK,EAAE,CAAC,QAAD,CAHI;;EAIXC,KAAK,CAACD,KAAD,EAAQ;IACT,MAAM;MAAEE,KAAF;MAASC;IAAT,IAAuBR,YAAY,CAAC,gBAAD,EAAmB,QAAnB,EAA6BK,KAAK,CAACI,MAAnC,CAAzC;IACA,MAAMC,WAAU,GAAIT,GAAG,CAACI,KAAK,CAACI,MAAP,CAAvB;IAEA,MAAME,QAAQ,GAACV,GAAG,CAAC,EAAD,CAAlB;;IAEA,MAAMW,WAAU,GAAKC,KAAD,IAAU;MAC1BF,QAAQ,CAACG,KAAT,GAAiBD,KAAjB;MACAE,OAAO,CAACC,GAAR,CAAY,oBAAZ,EAAkCL,QAAQ,CAACG,KAA3C;MACAH,QAAQ,CAACG,KAAT,CAAeL,MAAf,GAAwBJ,KAAK,CAACI,MAA9B;IAEJ,CALA;;IAOA,MAAMQ,UAAS,GAAIC,MAAD,IAAU;MACxBH,OAAO,CAACC,GAAR,CAAY,oBAAZ,EAAkCE,MAAlC;MACA,MAAMC,SAAQ,GAAIX,SAAS,CAACY,MAAV,CAAkBC,QAAD,IAAY;QAC3CA,QAAQ,CAACP,KAAT,CAAeQ,MAAf,IAAwBJ,MAAxB;MACH,CAFiB,CAAlB;IAGJ,CALA;;IASA,MAAMK,WAAW,GAAC,MAAI;MAClBZ,QAAQ,CAACG,KAAT,GAAe,EAAf;IACJ,CAFA;;IAMA,OAAO;MAAEJ,WAAF;MAAeH,KAAf;MAAsBC,SAAtB;MAAiCI,WAAjC;MAA8CD,QAA9C;MAAwDY,WAAxD;MAAqEN;IAArE,CAAP;EACJ;;AAjCW,CAAf","names":["ShowModule","getOrderDocs","ref","SingleModel","name","components","props","setup","error","documents","course","whichcourse","whichVid","showvidInfo","video","value","console","log","moveModule","theMod","newModule","filter","document","module","returnToAll"],"sourceRoot":"","sources":["/Users/docfrawley/Sites/NVoge/srsite/src/views/CourseView.vue"],"sourcesContent":["<template>\n    <div @click=\"returnToAll\" class=\"course\">\n        Course View: {{whichcourse}}\n    </div>\n    <div v-if=\"whichVid.order\">\n        <div class=\"module-list\">\n            <div v-for=\"document in documents\" :key=\"document.id\">\n                <span class=\"ind-mod\" @click=\"moveModule(document.module)\">{{document.title}} </span>\n            </div>\n        </div>\n        <SingleModel :specifics=\"whichVid\" />\n        <div class=\"next-previous\">\n            <div class=\"np-module\" v-if=\"whichVid.module>1\" @click=\"moveModule(whichVid.module-1)\">Previous Module</div>\n            <div class=\"np-module\" v-if=\"whichVid.module<documents.length\" @click=\"moveModule(whichVid.module+1)\">Next\n                Module</div>\n        </div>\n\n    </div>\n\n    <div v-else>\n        <div v-for=\"document in documents\" :key=\"document.id\">\n            <ShowModule :document=\"document\" @vidInfo=\"showvidInfo\" />\n        </div>\n    </div>\n\n</template>\n\n<script>\nimport ShowModule from '@/components/ShowModule.vue'\nimport getOrderDocs from '@/composables/getOrderDocs'\nimport { ref } from 'vue'\nimport SingleModel from '@/components/SingleModel.vue'\n\nexport default {\n    name: 'Courseview',\n    components: { ShowModule, SingleModel },\n    props: ['course'],\n    setup(props) {\n        const { error, documents } = getOrderDocs('course-modules', 'course', props.course)\n        const whichcourse = ref(props.course)\n        \n        const whichVid=ref({})\n        \n        const showvidInfo = (video) =>{\n            whichVid.value = video\n            console.log(\"here is the info: \", whichVid.value)\n            whichVid.value.course = props.course\n           \n        }\n\n        const moveModule =(theMod)=>{\n            console.log(\"Which module now: \", theMod)\n            const newModule = documents.filter((document)=>{\n                document.value.module ==theMod\n            })\n        }\n\n        \n\n        const returnToAll=()=>{\n            whichVid.value={}\n        }\n\n        \n\n        return { whichcourse, error, documents, showvidInfo, whichVid, returnToAll, moveModule}\n    }\n}\n</script>\n\n<style scoped>\n    .module-list{\n        display:flex;\n        flex-direction: row;\n        justify-content: space-around;\n    }\n    .ind-mod {\n        display: inline-block;\n        cursor: pointer;\n    }\n    .next-previous {\n        display: flex;\n        justify-content: center;\n    }\n    .np-module {\n        cursor: pointer;\n        margin: 0 15px;\n    }\n\n</style>"]},"metadata":{},"sourceType":"module"}