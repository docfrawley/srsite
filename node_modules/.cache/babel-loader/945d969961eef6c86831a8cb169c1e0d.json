{"ast":null,"code":"import ShowModule from '@/components/ShowModule.vue';\nimport getOrderDocs from '@/composables/getOrderDocs';\nimport getLesson from '@/composables/getLesson';\nimport { ref } from 'vue';\nimport SingleModel from '@/components/SingleModel.vue';\nexport default {\n  name: 'Courseview',\n  components: {\n    ShowModule,\n    SingleModel\n  },\n  props: ['course'],\n\n  setup(props) {\n    const {\n      error,\n      documents\n    } = getOrderDocs('course-modules', 'course', props.course);\n    const whichcourse = ref(props.course);\n    const whichVid = ref({});\n    const theNewMod = ref(null);\n\n    const showvidInfo = video => {\n      whichVid.value = video;\n      whichVid.value.course = props.course;\n    };\n\n    const moveModule = async theMod => {\n      whichVid.value.module = theMod; // const newModule= documents.value.filter((document)=>{\n      //     return document.module == theMod\n      // })\n      // theNewMod.value = {...newModule[0]}\n      // console.log(\"here is the new module: \", theNewMod.value.description)\n\n      const {\n        error: theerror,\n        document\n      } = await getLesson(whichcourse.value, theMod, 1);\n\n      if (!theerror.value) {\n        whichVid.value = document;\n        whichVid.value.course = whichcourse.value;\n        console.log(\"made it here: \", whichVid);\n      }\n    };\n\n    const returnToAll = () => {\n      whichVid.value = {};\n      theNewMod.value = null;\n    };\n\n    return {\n      whichcourse,\n      error,\n      documents,\n      showvidInfo,\n      whichVid,\n      returnToAll,\n      moveModule,\n      theNewMod\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAkCA,OAAOA,UAAP,MAAuB,6BAAvB;AACA,OAAOC,YAAP,MAAyB,4BAAzB;AACA,OAAOC,SAAP,MAAsB,yBAAtB;AACA,SAASC,GAAT,QAAoB,KAApB;AACA,OAAOC,WAAP,MAAwB,8BAAxB;AAEA,eAAe;EACXC,IAAI,EAAE,YADK;EAEXC,UAAU,EAAE;IAAEN,UAAF;IAAcI;EAAd,CAFD;EAGXG,KAAK,EAAE,CAAC,QAAD,CAHI;;EAIXC,KAAK,CAACD,KAAD,EAAQ;IACT,MAAM;MAAEE,KAAF;MAASC;I