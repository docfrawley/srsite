 // theNewMod.value = {...newModule[0]}\n            // console.log(\"here is the new module: \", theNewMod.value.description)\n            const { error: theerror, document } = await getLesson(whichcourse.value, theMod, 1)\n           if (!theerror.value){\n            document.course = whichcourse.value\n               whichVid.value = document\n            //    whichVid.value.course = whichcourse.value\n               console.log(\"made it here: \", whichVid, whichcourse.value, document.course)\n           }\n            \n        }\n\n        \n\n        const returnToAll=()=>{\n            whichVid.value=null\n            theNewMod.value=null\n        }\n\n        \n\n        return { whichcourse, error, documents, showvidInfo, whichVid, returnToAll, moveModule, theNewMod, check}\n    }\n}\n</script>\n\n<style scoped>\n    .module-list{\n        display:flex;\n        flex-direction: row;\n        justify-content: space-around;\n    }\n    .ind-mod {\n        display: inline-block;\n        cursor: pointer;\n    }\n    .next-previous {\n        display: flex;\n        justify-content: center;\n    }\n    .np-module {\n        cursor: pointer;\n        margin: 0 15px;\n    }\n\n</style>"]},"metadata":{},"sourceType":"module"}