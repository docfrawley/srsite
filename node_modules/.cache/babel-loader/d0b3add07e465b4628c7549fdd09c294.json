{"ast":null,"code":"import ShowModule from '@/components/ShowModule.vue';\nimport getOrderDocs from '@/composables/getOrderDocs';\nimport getLesson from '@/composables/getLesson';\nimport { ref, reactive } from 'vue';\nimport SingleModel from '@/components/SingleModel.vue';\nexport default {\n  name: 'Courseview',\n  components: {\n    ShowModule: ShowModule,\n    SingleModel: SingleModel\n  },\n  props: ['course'],\n  setup: function setup(props) {\n    var _getOrderDocs = getOrderDocs('course-modules', 'course', props.course),\n        error = _getOrderDocs.error,\n        documents = _getOrderDocs.documents;\n\n    var whichcourse = ref(props.course);\n    var whichVid = reactive();\n    var name = ref('John');\n\n    var showvidInfo = function showvidInfo(video) {\n      whichVid.value = video;\n      console.log('name: ', whichVid);\n    };\n\n    var moveModule = function moveModule(theMod) {\n      var _getLesson = getLesson(whichcourse.value, theMod, 1),\n          viderror = _getLesson.error,\n          newVideo = _getLesson.document;\n\n      if (newVideo) {\n        whichVid.value = newVideo;\n        console.log('here: ', whichVid.value.id);\n      }\n    };\n\n    var returnToAll = function returnToAll() {\n      whichVid.value = null;\n    };\n\n    return {\n      whichcourse: whichcourse,\n      error: error,\n      documents: documents,\n      showvidInfo: showvidInfo,\n      whichVid: whichVid,\n      returnToAll: returnToAll,\n      moveModule: moveModule\n    };\n  }\n};","map":{"version":3,"mappings":"AAqCA,OAAOA,UAAP,MAAuB,6BAAvB;AACA,OAAOC,YAAP,MAAyB,4BAAzB;AACA,OAAOC,SAAP,MAAsB,yBAAtB;AACA,SAASC,GAAT,EAAcC,QAAd,QAA8B,KAA9B;AACA,OAAOC,WAAP,MAAwB,8BAAxB;AAEA,eAAe;EACXC,IAAI,EAAE,YADK;EAEXC,UAAU,EAAE;IAAEP,UAAU,EAAVA,UAAF;IAAcK,WAAU,EAAVA;EAAd,CAFD;EAGXG,KAAK,EAAE,CAAC,QAAD,CAHI;EAIXC,KAJW,iBAILD,KAJK,EAIE;IACT,oBAA6BP,YAAY,CAAC,gBAAD,EAAmB,QAAnB,EAA6BO,KAAK,CAACE,MAAnC,CAAzC;IAAA,IAAQC,KAAR,iBAAQA,KAAR;IAAA,IAAeC,SAAf,iBAAeA,SAAf;;IACA,IAAMC,WA