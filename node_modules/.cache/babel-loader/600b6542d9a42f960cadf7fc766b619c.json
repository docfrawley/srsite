{"ast":null,"code":"import ShowModule from '@/components/ShowModule.vue';\nimport getOrderDocs from '@/composables/getOrderDocs';\nimport getLesson from '@/composables/getLesson';\nimport { ref } from 'vue';\nimport SingleModel from '@/components/SingleModel.vue';\nimport getCollection from '@/composables/getCollection';\nexport default {\n  name: 'Courseview',\n  components: {\n    ShowModule,\n    SingleModel\n  },\n  props: ['course'],\n\n  setup(props) {\n    const {\n      error,\n      documents\n    } = getOrderDocs('course-modules', 'course', props.course);\n    const {\n      error: errrr,\n      documents: megads\n    } = getCollection('courses');\n    console.log('megads: ', megads);\n    const whichcourse = ref(props.course);\n    const whichVid = ref({});\n    whichVid.value.iframe = '';\n    whichVid.value.module = null;\n    whichVid.value.course = whichcourse;\n\n    const showvidInfo = video => {\n      whichVid.value.iframe = video.iframe;\n      whichVid.value.module = video.module;\n    };\n\n    const moveModule = async theMod => {\n      whichVid.value.iframe = await getLesson(whichcourse.value, theMod, 1);\n      whichVid.value.module = theMod;\n    };\n\n    const returnToAll = () => {\n      whichVid.value.iframe = '';\n      whichVid.value.module = null;\n      whichVid.value.course = whichcourse;\n    };\n\n    return {\n      whichcourse,\n      error,\n      documents,\n      showvidInfo,\n      whichVid,\n      returnToAll,\n      moveModule\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAkCA,OAAOA,UAAP,MAAuB,6BAAvB;AACA,OAAOC,YAAP,MAAyB,4BAAzB;AACA,OAAOC,SAAP,MAAsB,yBAAtB;AACA,SAASC,GAAT,QAAoB,KAApB;AACA,OAAOC,WAAP,MAAwB,8BAAxB;AACA,OAAOC,aAAP,MAA0B,6BAA1B;AAEA,eAAe;EACXC,IAAI,EAAE,YADK;EAEXC,UAAU,EAAE;IAAEP,UAAF;IAAcI;EAAd,CAFD;EAGXI,KAAK,EAAE,CAAC,QAAD,CAHI;;EAIXC,KAAK,CAACD,KAAD,EAAQ;IACT,MAAM;MAAEE,KAAF;MAASC;IAAT,IAAuBV,YAAY,CAAC,gBAAD,EAAmB,QAAnB,EAA6BO,KAAK,CAACI,MAAnC,CAAzC;IACA,MAAM;MAAEF,KAAK,EAAEG,KAAT;MAAgBF,SAAS,EAAEG