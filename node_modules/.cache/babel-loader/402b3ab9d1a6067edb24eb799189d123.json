<div>\n      ADD Video +\n    </div>\n  </div>\n\n  <div v-if=\"currentVideo\">\n    <VideoForm :videoInfo=\"currentVideo\" :courseCol=\"currentCourse.col_name\" />\n  </div>\n\n\n\n</template>\n\n<script>\nimport { ref, reactive } from 'vue'\nimport getCollection from '@/composables/getCollection'\nimport CourseForm from '@/components/CourseForm.vue'\nimport ModuleForm from '@/components/ModuleForm.vue'\nimport VideoForm from '@/components/VideoForm.vue'\n\nexport default {\n  components: {CourseForm, ModuleForm, VideoForm },\n setup() {\n  const currentCourse = reactive({\n    id:'',\n    title:'',\n    col_name:'',\n    descrition:'',\n    instructor:'',\n    launch:'',\n    status: ''\n  })\n  const currentModule = ref()\n  const currentVideo = ref()\n  const allvideos = ref()\n  const { error, documents: courses} = getCollection('courses')\n\n  const goToCourse = (c) => {\n    currentCourse.value = c\n    console.log(\"c: \", c.id)\n  }\n\n  \n\n  const showVid = (info)=>{\n    if (info){\n      currentVideo.value = info\n    }\n    \n  }\n\n  const newCourseForm=()=>{\n      currentCourse.value = \"new_course\"\n  }\n\n\n  return { goToCourse, courses, currentCourse, currentModule, showVid, newCourseForm, allvideos, currentVideo}\n }\n}\n</script>\n\n<style scoped>\n.course-container, .module-container, .video-container{\n  display:flex;\n  justify-content: space-between;\n  flex-direction: row;\n}\n.course-listing{\n  width:250px;\n  height:300px;\n  background-color: bisque;\n  cursor: pointer;\n  margin: 10px;\n}\n\n\n\n.video-listing{\n  padding:10px;\n  background-color:lightblue;\n  cursor: pointer;\n  margin: 10px;\n}\n\n\n</style>"]},"metadata":{},"sourceType":"module"}