{"ast":null,"code":"import { ref, reactive } from 'vue';\nimport getCollection from '@/composables/getCollection';\nimport CourseForm from '@/components/CourseForm.vue';\nimport ModuleForm from '@/components/ModuleForm.vue';\nimport VideoForm from '@/components/VideoForm.vue';\nexport default {\n  components: {\n    CourseForm: CourseForm,\n    ModuleForm: ModuleForm,\n    VideoForm: VideoForm\n  },\n  setup: function setup() {\n    var currentCourse = reactive({\n      id: '',\n      title: '',\n      col_name: '',\n      descrition: '',\n      instructor: '',\n      launch: '',\n      status: ''\n    });\n    var currentModule = ref();\n    var currentVideo = ref();\n    var allvideos = ref();\n\n    var _getCollection = getCollection('courses'),\n        error = _getCollection.error,\n        courses = _getCollection.documents;\n\n    var goToCourse = function goToCourse(c) {\n      currentCourse.value = c;\n      console.log(\"c: \", c.id);\n    };\n\n    var showVid = function showVid(info) {\n      if (info) {\n        currentVideo.value = info;\n      }\n    };\n\n    var newCourseForm = function newCourseForm() {\n      currentCourse.value = \"new_course\";\n    };\n\n    return {\n      goToCourse: goToCourse,\n      courses: courses,\n      currentCourse: currentCourse,\n      currentModule: currentModule,\n      showVid: showVid,\n      newCourseForm: newCourseForm,\n      allvideos: allvideos,\n      currentVideo: currentVideo\n    };\n  }\n};","map":{"version":3,"mappings":"AAsCA,SAASA,GAAT,EAAcC,QAAd,QAA8B,KAA9B;AACA,OAAOC,aAAP,MAA0B,6BAA1B;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AACA,OAAOC,SAAP,MAAsB,4BAAtB;AAEA,eAAe;EACbC,UAAU,EAAE;IAACH,UAAU,EAAVA,UAAD;IAAaC,UAAU,EAAVA,UAAb;IAAyBC,SAAQ,EAARA;EAAzB,CADC;EAEdE,KAFc,mBAEN;IACP,IAAMC,aAAY,GAAIP,QAAQ,CAAC;MAC7BQ,EAAE,EAAC,EAD0B;MAE7BC,KAAK,EAAC,EAFuB;MAG7BC,QAAQ,EAAC,EAHoB;MAI7BC,UAAU,EAAC,EAJkB;MAK7BC,UAAU,EAAC,EALkB;MAM7BC,MAAM,EAAC,EANsB;MAO7BC,MAAM,EAAE;IAPqB,CAAD,CAA9B;IASA,IAAMC,a