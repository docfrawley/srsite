{"ast":null,"code":"import ShowModule from '@/components/ShowModule.vue';\nimport getOrderDocs from '@/composables/getOrderDocs';\nimport getLesson from '@/composables/getLesson';\nimport { ref } from 'vue';\nimport SingleModel from '@/components/SingleModel.vue';\nexport default {\n  name: 'Courseview',\n  components: {\n    ShowModule,\n    SingleModel\n  },\n  props: ['course'],\n\n  setup(props) {\n    const {\n      error,\n      documents\n    } = getOrderDocs('course-modules', 'course', props.course);\n    const check = ref(0);\n    const whichcourse = ref(props.course);\n    const whichVid = ref({});\n    const theNewMod = ref(null);\n    whichVid.value.iframe = '';\n    whichVid.value.module = null;\n    whichVid.value.course = whichcourse;\n\n    const showvidInfo = video => {\n      whichVid.value.iframe = video.iframe;\n      whichVid.value.module = video.module;\n    };\n\n    const moveModule = async theMod => {\n      check.value = theMod; // const newModule= documents.value.filter((document)=>{\n      //     return document.module == theMod\n      // })\n      // theNewMod.value = {...newModule[0]}\n      // console.log(\"here is the new module: \", theNewMod.value.description)\n\n      const {\n        error: theerror,\n        document\n      } = await getLesson(whichcourse.value, theMod, 1);\n\n      if (!theerror.value) {\n        whichVid.value.iframe = document.iframe;\n        whichVid.value.module = document.module; //    whichVid.value.course = whichcourse.value\n\n        console.log(\"made it here: \", whichVid);\n      }\n    };\n\n    const returnToAll = () => {\n      whichVid.value = null;\n      theNewMod.value = null;\n    };\n\n    return {\n      whichcourse,\n      error,\n      documents,\n      showvidInfo,\n      whichVid,\n      returnToAll,\n      moveModule,\n      theNewMod,\n      check\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAsCA,OAAOA,UAAP,MAAuB,6BAAvB;AACA,OAAOC,YAAP,MAAyB,4BAAzB;AACA,OAAOC,SAAP,MAAsB,yBAAtB