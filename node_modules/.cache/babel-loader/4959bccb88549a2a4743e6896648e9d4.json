div>\n        checking {{check}}\n    </div>\n    <div v-if=\"whichVid\">\n\n        here is the whichVid {{whichVid}}\n       \n    </div>\n\n    <div v-if=\"whichVid\">\n        <div class=\"module-list\">\n            <div v-for=\"document in documents\" :key=\"document.id\">\n                <span class=\"ind-mod\" @click=\"moveModule(document.module)\">{{document.title}} </span>\n            </div>\n        </div>\n        \n        <SingleModel :specifics=\"whichVid\" />\n        <div class=\"next-previous\">\n            <div class=\"np-module\" v-if=\"whichVid.module>1\" @click=\"moveModule(whichVid.module-1)\">Previous Module</div>\n            <div class=\"np-module\" v-if=\"whichVid.module<documents.length\" @click=\"moveModule(whichVid.module+1)\">Next\n                Module</div>\n        </div>\n\n    </div>\n\n    <div v-else>\n        <div v-for=\"document in documents\" :key=\"document.id\">\n            <ShowModule :document=\"document\" @vidInfo=\"showvidInfo\" />\n        </div>\n    </div>\n\n</template>\n\n<script>\nimport ShowModule from '@/components/ShowModule.vue'\nimport getOrderDocs from '@/composables/getOrderDocs'\nimport getLesson from '@/composables/getLesson'\nimport { ref } from 'vue'\nimport SingleModel from '@/components/SingleModel.vue'\n\nexport default {\n    name: 'Courseview',\n    components: { ShowModule, SingleModel },\n    props: ['course'],\n    setup(props) {\n        const { error, documents } = getOrderDocs('course-modules', 'course', props.course)\n        const check = ref(0)\n        const whichcourse = ref(props.course)\n        const whichVid = ref({})\n        const theNewMod = ref(null)\n\n        whichVid.value.iframe=''\n        whichVid.value.module=null\n        whichVid.value.course = whichcourse\n\n        const showvidInfo = (video) =>{\n            whichVid.value.iframe = video.iframe\n            whichVid.value.module = video.module\n           \n        }\n\n        const moveModule = async (theMod)=>{\n            \n            check.value = theMod\n            // const newModule= documents.value.filter((document)=>{\n            //     return document.module == theMod\n              \n            // })\n            // theNewMod.value = {...newModule[0]}\n            // console.log(\"here is the new module: \", theNewMod.value.description)\n            const { error: theerror, document } = await getLesson(whichcourse.value, theMod, 1)\n           if (!theerror.value){\n               whichVid.value.iframe = document.iframe\n               whichVid.value.module = document.module\n            //    whichVid.value.course = whichcourse.value\n               console.log(\"made it here: \", whichVid)\n           }\n            \n        }\n\n        \n\n        const returnToAll=()=>{\n            whichVid.value=null\n            theNewMod.value=null\n        }\n\n        \n\n        return { whichcourse, error, documents, showvidInfo, whichVid, returnToAll, moveModule, theNewMod, check}\n    }\n}\n</script>\n\n<style scoped>\n    .module-list{\n        display:flex;\n        flex-direction: row;\n        justify-content: space-around;\n    }\n    .ind-mod {\n        display: inline-block;\n        cursor: pointer;\n    }\n    .next-previous {\n        display: flex;\n        justify-content: center;\n    }\n    .np-module {\n        cursor: pointer;\n        margin: 0 15px;\n    }\n\n</style>"]},"metadata":{},"sourceType":"module"}